        <activity android:name=".view.LoginActivity" />

        <activity android:name=".view.ChatActivity" />

        <activity
            android:name=".view.DetailItemActivity"
            android:exported="true" />

        <activity android:name=".view.CreateItemActivity" />

        <activity android:name=".view.EditProfileActivity" />

        <activity android:name=".view.SignUpActivity" />

        <activity android:name=".view.MainActivity" />

        <activity
            android:name=".view.LoadingActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>