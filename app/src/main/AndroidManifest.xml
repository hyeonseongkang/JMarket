<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mirror.jmarket">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JMarket">

        <activity android:name=".view.MyBuyListActivity" />

        <activity android:name=".view.MySalesListActivity" />

        <activity android:name=".view.MyInterestListActivity" />

        <activity android:name=".view.ReviewActivity" />

        <activity
            android:name=".view.ChatMenuActivity"
